{
  "name": "printify",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "dev": "meteor run --settings settings-development.json",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "test:collections": "meteor test --once --driver-package meteortesting:mocha --grep collections",
    "test:methods": "meteor test --once --driver-package meteortesting:mocha --grep methods",
    "test:integrations": "meteor test --once --driver-package meteortesting:mocha --grep integrations",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "build": "meteor build ../build --directory",
    "docker:build": "docker build -t printify:latest .",
    "docker:run": "docker-compose up"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.5",
    "@swc/helpers": "^0.5.17",
    "meteor-node-stubs": "^1.2.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simpl-schema": "^3.4.6",
    "bcrypt": "^5.1.1",
    "fabric": "^5.3.0",
    "sharp": "^0.33.1",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "react-router-dom": "^6.20.1",
    "react-dropzone": "^14.2.3",
    "react-toastify": "^9.1.3",
    "body-parser": "^1.20.2",
    "date-fns": "^3.0.6",
    "lodash": "^4.17.21",
    "react-icons": "^4.12.0",
    "react-hook-form": "^7.49.2",
    "recharts": "^2.10.3",
    "framer-motion": "^10.16.16",
    "@headlessui/react": "^1.7.17",
    "konva": "^9.2.0",
    "react-konva": "^18.2.10",
    "use-image": "^1.1.1"
  },
  "devDependencies": {
    "@types/meteor": "^2.9.8",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.32",
    "autoprefixer": "^10.4.16",
    "@tailwindcss/forms": "^0.5.7"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.jsx",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js",
    "modern": true
  }
}
